{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"Color Update",
    "description":"change the bg color of a website to any color",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["color.js"]   
        }
    ],
    "action":{
        "default_popup":"popup.html"
    }
}
    